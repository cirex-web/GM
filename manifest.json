{
    "manifest_version": 2,
    "name": "GM",
    "version":"0.1",
    "browser_action":{
        "default_popup": "/popup/popup.html"
    },
    "permissions":["activeTab","storage"],
    "content_scripts":[
        {
            "js":["/external/jquery.js","/external/arrive.js","content.js"],
            "matches": [
                "*://meet.google.com/*"
            ]
        }
    ],
    "content_security_policy": "style-src 'self' https://kit.fontawesome.com; 'unsafe-inline'",
    "web_accessible_resources": [
        "script.js","/external/jquery.js","/external/arrive.js","https://kit.fontawesome.com","/popup/popup.js","/external/moment.js"
      ]
 
}